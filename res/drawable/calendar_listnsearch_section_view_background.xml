<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item>
        <shape android:shape="rectangle" >
            <solid android:color="@color/black" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle" >
            <solid android:color="#A5B1BA" />
        </shape>
    </item>
    <item android:top="1px">
        <shape android:shape="rectangle" >
            <solid android:color="#98A3AA" />
        </shape>
    </item>
    <item
        android:bottom="1px"
        android:top="1px">
        <shape android:shape="rectangle" >
            <gradient
                android:angle="270"
                android:endColor="#B7C0C7"
                android:startColor="#94A2AD" />
        </shape>
    </item>

</layer-list>