<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/calendar_listnsearch_list_selector_background"
    android:cacheColorHint="#00000000"
    android:clickable="true"
    android:orientation="horizontal"
    android:padding="2dp"
    android:scrollingCache="false" >

    <ImageView
        android:id="@+id/listnsearch_entry_color_placeholder"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical|left"
        android:layout_marginBottom="3dp"
        android:layout_marginRight="3dp"
        android:layout_marginTop="3dp"
        android:cacheColorHint="#00000000"
        android:contentDescription="@string/event_icons_placeholder"
        android:scrollingCache="false"
        android:src="@null" />

    <ImageView
        android:id="@+id/listnsearch_entry_icon_placeholder"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_gravity="center_vertical|left"
        android:layout_marginBottom="3dp"
        android:layout_marginRight="3dp"
        android:layout_marginTop="3dp"
        android:cacheColorHint="#00000000"
        android:contentDescription="@string/event_icons_placeholder"
        android:scrollingCache="false"
        android:src="@null" />

